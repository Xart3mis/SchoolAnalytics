generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id              String   @id @default(cuid())
  fullName        String
  gradeLevel      String
  gpa             Float
  attendanceRate  Float
  outstandingFees Float
  createdAt       DateTime @default(now())
}
